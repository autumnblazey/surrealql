{
	"name": "surrealql",
	"version": "0.0.0",
	"description": "small SurrealDB JS/TS library to help facilitate the use of SurrealQL",
	"packageManager": "pnpm@7.11.0",
	"main": "build/index.js",
	"module": "build/index.js",
	"types": "build/index.d.ts",
	"type": "module",
	"sideEffects": false,
	"files": [
		"build"
	],
	"scripts": {
		"build": "tsc",
		"test": "NODE_OPTIONS=\"--experimental-specifier-resolution=node --loader ts-node/esm\" nyc --reporter json mocha"
	},
	"engines": {
		"node": ">=16"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/autumnblazey/surrealql.git"
	},
	"keywords": [
		"surrealdb",
		"surrealql",
		"surreal",
		"sql"
	],
	"author": {
		"name": "Autumn Meadow",
		"url": "https://github.com/autumnblazey"
	},
	"license": "MIT",
	"dependencies": {
		"ohmyfetch": "^0.4.18"
	},
	"devDependencies": {
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.7.17",
		"mocha": "^10.0.0",
		"nyc": "^15.1.0",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	}
}
